jQuery(function($){JBZoo.widget('HyperPC.Geo.Phone',{'dadataApiKey':''},{init:function($this){$this._subscribeGeoEvents($this);$this._defineUserLocation($this,$this.getOption('dadataApiKey'));},_onLocationDefined:function(){var userLocation=this._getUserLocation(this);if(userLocation.geoId==213||userLocation.country!=='Россия'){this.$('.jsPhoneMoscow').removeClass('uk-hidden');this.$('.jsPhoneRussia').addClass('uk-hidden');}else{this.$('.jsPhoneRussia').removeClass('uk-hidden');this.$('.jsPhoneMoscow').addClass('uk-hidden');}}});});
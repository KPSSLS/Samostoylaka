function deferCss(cssFile,timeout){var link=document.createElement('link');link.rel='stylesheet';link.href=cssFile;link.type='text/css';var timeout=timeout||0;setTimeout(function(){var linkTag=document.getElementsByTagName('link')[0];linkTag.parentNode.insertBefore(link,linkTag);},timeout);}function deferJs(jsFile,cb,timeout){var script=document.createElement('script');script.src=jsFile;var timeout=timeout||0;setTimeout(function(){var scriptTag=document.getElementsByTagName('script')[0];scriptTag.parentNode.insertBefore(script,scriptTag);},timeout);if(typeof cb==='function'){script.addEventListener("load",function(e){setTimeout(function(){cb();},50);});}}document.addEventListener('DOMContentLoaded',function(){var $window=jQuery(window),paths=Joomla.getOptions('deferedPaths');if(document.querySelector('body').classList.contains('device-mobile-no')){if(paths.mainnavJs&&document.querySelector('.tm-nav-main')){deferJs(paths.mainnavJs,function(){jQuery('.uk-navbar-nav','.tm-nav-main').HyperPCMainnav({});},100);}}if(paths.product360Js&&document.querySelector('[data-product-360]')){deferJs(paths.product360Js,function(){$window.HyperPCProduct360&&jQuery('[data-product-360]').HyperPCProduct360({});});}if(paths.geoPhoneJs&&document.querySelector('.jsGeoPhones')){deferJs(paths.geoPhoneJs,function(){$window.HyperPCGeoPhone&&jQuery('.jsGeoPhones').HyperPCGeoPhone({'dadataApiKey':window.dadataToken||''});});}if(paths.geoPhoneInputJs&&document.querySelector('input[type="tel"]')){deferJs(paths.geoPhoneInputJs,function(){$window.HyperPCGeoPhoneInput&&jQuery('input[type="tel"]').HyperPCGeoPhoneInput({'dadataApiKey':window.dadataToken||''});});}if(paths.separatedNavbarJs&&document.querySelector('.jsSeparatedNavbar')){deferJs(paths.separatedNavbarJs,function(){jQuery('.jsSeparatedNavbar').HyperPCSeparatedNavbar({});});}if(paths.scrollableNavJs&&document.querySelector('.jsScrollableNav')){deferJs(paths.scrollableNavJs,function(){jQuery('.jsScrollableNav').HyperPCScrollableNav({});});}if(paths.scrollableListJs&&document.querySelector('.jsScrollableList')){deferJs(paths.scrollableListJs,function(){jQuery('.jsScrollableList').HyperPCScrollableList({});});}if(paths.labelInfieldJs&&document.querySelector('.tm-label-infield')){deferJs(paths.labelInfieldJs,function(){jQuery('.tm-label-infield').HyperPCLabelInfield({});});}paths.formHelperJs&&deferJs(paths.formHelperJs,null,200);paths.loadIframeJs&&deferJs(paths.loadIframeJs,function(){jQuery('body').HyperPCLoadIframe({});},500);if(paths.detailToggleJs&&document.querySelector('.jsDetailToggle')){deferJs(paths.detailToggleJs,function(){jQuery('.jsDetailToggle').HyperPCDetailToggle({});},1000);}paths.suggestionsMinCss&&deferCss(paths.suggestionsMinCss);if(paths.scrollToTopJs&&window.innerWidth>1665&&document.querySelector('.jsToTopButton')){deferJs(paths.scrollToTopJs,function(){jQuery('.jsToTopButton').HyperPCScrollToTop({});},3000);}});
jQuery(function($){JBZoo.widget('HyperPC.SiteBuyNow',{},{init:function($this){},_setFormData:function($this,data){var $modal=$('.jsBuyNowModal');var $form=$modal.find('form');$this._clearFormData($form);if(typeof data==='object'){var $inputs=$form.find('[name^="hp-item_"]');for(var prop in data){$inputs.filter('[name^="hp-item_'+prop+'"]').val(data[prop]);}}},_clearFormData:function($form){$form.find('[name^="hp-item_"]').val('');},'click {element}':function(e,$this){var data=$this.el.data('itemInfo');$this._setFormData($this,data);}});});